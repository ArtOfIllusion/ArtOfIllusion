<?xml version="1.0" standalone="yes"?>
<extension name="Renderers" version="2.5">
  <author>Peter Eastman</author>
  <date>2007</date>
  <description>
This plugin contains the standard renderers (Raster and Raytracer) which are included with Art of Illusion.
  </description>
  <comments>
  </comments>
  <plugin class="artofillusion.raster.Raster">
    <proxy interface="artofillusion.Renderer">
      <method name="getName" value="Raster"/>
    </proxy>
  </plugin>
  <plugin class="artofillusion.raytracer.Raytracer">
    <proxy interface="artofillusion.Renderer">
      <method name="getName" value="Raytracer"/>
    </proxy>
  </plugin>
</extension>

